    "aws-mqtt-client": "0.0.5",
    "aws-sdk": "^2.253.1",
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "chartjs-plugin-annotation": "^0.5.7",
    "leaflet": "^1.3.3",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "leaflet-geometryutil": "^0.9.0",
    "leaflet-rotatedmarker": "^0.2.0",
    "moment": "^2.22.2",
    "simple-web-worker": "^1.2.0",
    "vue": "^2.5.16",
    "vue-chartjs": "^3.3.2",
    "vue-router": "^3.0.1",
    "vue2-circle-progress": "^1.2.3",
